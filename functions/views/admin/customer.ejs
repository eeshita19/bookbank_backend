        </div>

    </div>
    <div class='container1' id='content2'>
        <h3>Users</h3>
        <div class='container1' id='tablecontent'>
            <p id='pending'>Users who submitted details</p>
            <!-- <input type='button' class='btn' id='filter' value='Filter'> -->
            <table class='table table-striped table-condensed' id='users-table'>
                <thead>
                    <tr>
                        <th>Users</th>
                        <th>Date Applied</th>
                        <th>Year</th>
                        <th>Course</th>
                        <th>Institute</th>
                        <th>City</th>
                        <th>Referred</th>
                    </tr>
                </thead>
            </table>

        </div>
    </div>
</div>

