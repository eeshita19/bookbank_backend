<%- include ('../partials/admin_header') %>

<div class='col-md-8' id='content'>
    <div class='row'>
        <div class='container1' id='totalusers'>TotalUsers<br /><span id='number'>3045</span><br /><span
                id='percentage'>+3.88%</span> <span id='month'>from last month</span>
            <img id='uparrow' class='images' src='/images/admin/Group%2075.png'>
        </div>
        <div class='container1' id='newusers' style='background-color: #1A95FF;color: white'>New Users<br /><span
                id='number' style='color: white'>147</span><br /><span id='percentage2'
                style='color: white'>+3.88%</span> <span id='month'>from last month</span>
            <img id='downarrow' class='images' src='/images/admin/Group%2077white.png'>
        </div>
        <div class='container1' id='sales'>Sales<br /><span id='number'>134</span><br /><span id='percentage'
