<%- include ("../partials/admin_header") %>

<div class="container1" id="userinfocontent">
    <div id="allcontent">
        <img id="userimage" src="/images/admin/Mask%20Group%201@2x.png">
        <h4> <%= username %></h4>
        <p id="residence">Default</p>
        <input type="button" onclick="acceptReview()" class="btn" id="accept" value="Accept">
        <input type="button" onclick="rejectReview()" class="btn" id="reject" value="Reject">

        <input type="button" style="display: none;" class="btn" id="accepted" value="Accepted" disabled>
        <input type="button" style="display: none;" class="btn" id="rejected" value="Rejected" disabled>

        <input type="button" onclick="reconsiderReview()" style="display: none;" class="btn" id="reconsider"
            value="Reconsider">

        <hr class="hrlines">

        <p id="year">Default<br />
            <span id="admissionyear">Default year</span></p>
        <p id="third">Default<br />
            <span id="currentsemester">Default year</span></p>
        <p id="morning"> <br />
            <span id="shift">Default year</span></p>
        <hr class="hrlines">
        <p id="branchname">Default<br /> </p>
        <p>
            <span id="collegename">Default Tech</span>
        </p>
    </div>
</div>


<div class="container1" id="userdetailcontent">
    <div class="carousel slide" id="mycarousel" data-ride="carousel">
        <ol class="carousel-indicators">
            <li class="active" data-target="#mycarousel" data-slide-to="0"></li>
            <li data-target="#mycarousel" data-slide-to="1"></li>
            <li data-target="#mycarousel" data-slide-to="2"></li>

        </ol>
        <div class="carousel-inner">
            <div class="carousel-item">
                <p id="personal">Personal Data</p>
                <input type="button" class="btn" onclick="goToUserInfo1()" id="next" value="Next">
                <hr id="straight">
                <div class="form-group col-md-6">
                    <label for="mobileno" style="color: #172B4D">Mobile</label>
                    <input type="text" class="form-control" value="+91 Default" id="mobileno"
                        style="color: #3bf127;background-color: white; border: 1px solid #3bf127; box-shadow: 1px 2px 5px #8898AA">
                </div>
                <div class="form-group col-md-6">
                    <label for="email" style="color: #172B4D">Email address</label>
                    <input type="email" class="form-control" value="Default@gmail.com" id="email"
                        style="color: red;background-color: white; border: 1px solid red ; box-shadow: 1px 2px 5px #8898AA">
                    <p id="notverified"> <%= state %> </p>
                </div>
                <br />
                <div class="form-group col-md-6">
                    <label for="user1" style="color: #172B4D">First Name</label>
                    <input type="text" class="form-control" value="Default" id="user1"
                        style="color: #8898AA;background-color: #F7FAFC; border: 1px; box-shadow: 1px 2px 5px">
                </div>
                <div class="form-group col-md-6">
                    <label for="lastname" style="color: #172B4D">Last Name</label>
                    <input type="text" class="form-control" value="Default name" id="lastname"
                        style="color: #8898AA;background-color: #F7FAFC; border: 1px; box-shadow: 1px 2px 5px">
                </div>
                <br />
                <div class="form-group col-md-6">
                    <label for="fathername" style="color: #172B4D">father's First Name</label>
                    <input type="text" class="form-control" value="Default's father" id="fathername"
                        style="color: #8898AA;background-color: #F7FAFC; border: 1px; box-shadow: 1px 2px 5px">
                </div>
                <div class="form-group col-md-6">
                    <label for="lastname" style="color: #172B4D">Last Name</label>
                    <input type="text" class="form-control" value="Default name" id="fatherlastname"
                        style="color: #8898AA;background-color: #F7FAFC; border: 1px; box-shadow: 1px 2px 5px">
                </div>
                <br />
                <div class="form-group col-md-4">
                    <label for="state" style="color: #172B4D">State</label>
                    <input type="text" class="form-control" value="Default" id="state"
                        style="color: #8898AA;background-color: #F7FAFC; border: 1px; box-shadow: 1px 2px 5px">
                </div>
                <div class="form-group col-md-4">
                    <label for="city" style="color: #172B4D">City</label>
                    <input type="text" class="form-control" value="Default" id="city"
                        style="color: #8898AA;background-color: #F7FAFC; border: 1px; box-shadow: 1px 2px 5px">
                </div>
                <div class="form-group col-md-4">
                    <label for="dob" style="color: #172B4D">Date of Birth</label>
                    <input type="text" class="form-control" value="Default" id="dob"
                        style="color: #8898AA;background-color: #F7FAFC; border: 1px; box-shadow: 1px 2px 5px">
                </div>
            </div>
        </div>
    </div>
</div>
<script src="/scripts/admin/commonInfo.js" defer></script>
<script src="/scripts/admin/userinfo.js" defer></script>

<%- include ("../partials/admin_footer") %>