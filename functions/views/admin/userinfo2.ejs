<%- include ("../partials/admin_header") %>
<div class="container1" id="userinfocontent">
    <div id="allcontent">
        <img id="userimage" src="/images/admin/Mask%20Group%201@2x.png">
        <h4> <%= username %></h4>
        <p id="residence">Default</p>
        <input type="button" onclick="acceptReview()" class="btn" id="accept" value="Accept">
        <input type="button" onclick="rejectReview()" class="btn" id="reject" value="Reject">

        <input type="button" style="display: none;" class="btn" id="accepted" value="Accepted" disabled>
        <input type="button" style="display: none;" class="btn" id="rejected" value="Rejected" disabled>

        <input type="button" onclick="reconsiderReview()" style="display: none;" class="btn" id="reconsider"
            value="Reconsider">

        <hr class="hrlines">

        <p id="year">Default<br />
            <span id="admissionyear">Default year</span></p>
        <p id="third">Default<br />
            <span id="currentsemester">Default year</span></p>
        <p id="morning"> <br />
            <span id="shift">Default year</span></p>
        <hr class="hrlines">
        <p id="branchname">Default<br /> </p>
        <p>
            <span id="collegename">Default Tech</span>
        </p>
    </div>
</div>
<div class="container1" id="userdetailcontent">
    <div class="carousel slide" id="mycarousel" data-ride="carousel">
        <ol class="carousel-indicators">
            <li data-target="#mycarousel" data-slide-to="0"></li>
            <li data-target="#mycarousel" data-slide-to="1"></li>
            <li class="active" data-target="#mycarousel" data-slide-to="2"></li>
        </ol>
    </div>
    <div class="corousel-item active">
        <p id="submission">Submission</p>
        <input type="button" onclick="goToUserInfo1();" class="btn" id="previous" value="Previous">
        <hr id="straight1">

        <div class="form-group col-md-12">
            <label for="photo" style="color: #172B4D">Photo</label>
            <input type="text" class="form-control" value="Passport_photo.jpg" id="photo"
                style="background-color: #F7FAFC;color: #8898AA;  box-shadow: 1px 2px 5px #8898AA">
            <img src="/images/admin/icons8-eye-100.png" id="eyeforeye">
        </div>
        <br />
        <br />
        <div class="form-group col-md-12">
            <label for="user1" style="color: #172B4D">Upload ID</label>
            <input type="text" class="form-control" value="Admissionslip.JPG" id="userId"
                style="color: #8898AA;background-color: #F7FAFC; border: 1px; box-shadow: 1px 2px 5px">
        </div>
        <br />
        <br />
        <div class="form-group col-md-12">
            <label for="fathername" style="color: #172B4D">Objection if you have</label>
            <textarea class="form-control" id="objection" placeholer="Please default write here..."
                style="color: #8898AA; background-color: #F7FAFC; border: 1px; box-shadow: 1px 2px 5px; height: 110px; "></textarea>
            <input type="button" onclick="saveObjection();" class="btn" id="previous" value="Submit">
        </div>
        <br />
    </div>
</div>
<div class="container1" id="rejectiondiv" hidden>
    <h5>Are you sure?</h5>
    <p>You won't be able to revert this action!</p>
    <input type="button" class="btn" id="yesreject" value="Yes,Reject">
    <input type="button" class="btn" id="cancel" value="Cancel">

</div>
<script src="/scripts/admin/commonInfo.js" defer></script>
<script src="/scripts/admin/userinfo2.js" defer></script>

<%- include ("../partials/admin_footer") %>