                <li id="list3"><button id="button2"><img src="/cs/Images/icons8-notification-100.png"
                            id="bell"></button><span id="bellnot"><img src="/cs/Images/Ellipse%2045.png"></span></li>
                <li id="list4"><button id="button3"><img src="/cs/Images/icons8-logout-rounded-down-100.png"
                            id="logout"></button></li>

            </ul>

        </div>


    </div>
    <div id="left">
        <img id="brandname" src="/cs/Images/monktree%20white.png">
        <div id="listdiv">
            <ul>
                <li><a href="dashboard.html"><img src="/cs/Images/Group%2032.png" id="dashboard">Dashboard</a></li>
                <li><a href="yourbag.html"><img src="/cs/Images/Group%2074.png" id="bag">Your Bag</a></li>
                <li><a href="#"><img src="/cs/Images/Group%2062.png" id="gift">Free gifts</a></li>
                <li id="notestutorial"><a href="#"><img src="/cs/Images/Group%2063.png" id="notes">Notes&Tutorials</a>
                </li>
                <li id="codeplug"><a href="#">Codeplug IDE</a></li>
            </ul>

        </div>
        <hr id="line">
        <div id="listdiv2">
            <ul>
                <li><a href="#">Discussion</a></li>
                <li id="contact"><a href="#">Contact</a></li>
                <li><a href="#">About</a></li>
            </ul>
        </div>
    </div>
    <div id="container">
        <div id="notactive">Not Active</div>
        <div id="containsform">
            <div class="row2">
                <div class="column2 left2">
                    <a href="personaldetails.html"><button class="button5">Personal Details</button></a>
                </div>
                <div class="column2 mid2">
                    <a href="academics.html"><button class="button5" style="margin-left: 0px;">Academic
                            Details</button></a>
                </div>
                <div class="column2 right2">
                    <a href="submission.html"><button class="button5" style="margin-left: 8px;">Submission</button></a>
                </div>
                <div class="container1">
                    <form method="post" action="/Academics.html">
                        <label id="">Student's name</label>
                        <input type="" name="" class="textbox1" placeholder=" First name" style="margin-left: 30px;"
                            required>&nbsp&nbsp
                        <input type="" name="" class="textbox1" placeholder=" Middle name">&nbsp&nbsp
                        <input type="" name="" class="textbox1" placeholder=" Last name" required>
                        <br><br>

                        <label id="">Father's name</label>
                        <input type="" name="" class="textbox1" placeholder=" First name" style="margin-left: 40px;"
                            required>&nbsp&nbsp
                        <input type="" name="" class="textbox1" placeholder=" Last name" required>
                        <br><br>

                        <label id="">Date of Birth</label>
                        <input type="date" name="" class="textbox1" placeholder=" DD/MM/YYYY"
                            style="margin-left: 51px;">&nbsp&nbsp

                        <br><br>

                        <label id="">Email</label>
                        <input id="email" type="email" name="" class="textbox1" disabled
                            style="margin-left: 103px; padding-left: 5px;" value="reply@student.in">&nbsp&nbsp
                        <div id="status"
                            style="width: 80px;height: 15px;background-color:#6ff55b;position: absolute;left: 450px; bottom: 249px;border-radius: 0px;color: white;padding: 10px;font-weight: bold">
                            VERIFIED</div>
                        <input type="button" value="Change" id="change"
                            style="width: 80px;height: 36px;background-color:#f5f5f5;position: absolute;left: 555px; bottom: 249px;border-radius: 0px;color: grey;padding: 10px;font-weight: bold;border: 1px;font-size: 15px">

                        <br><br>

                        <label id="">Mobile</label>
                        <input id="mobile" value="9886338488" type="phone" name="" class="textbox1" disabled
                            style="margin-left: 94px;padding: 6px">&nbsp&nbsp
                        <div id="statusmobile"
                            style="width: 80px;height: 15px;background-color:#6ff55b;position: absolute;left: 450px; bottom: 192px;border-radius: 0px;color: white;padding: 10px;font-weight: bold">
                            VERIFIED</div>
                        <input type="button" value="Change" id="changemobile"
                            style="width: 80px;height: 36px;background-color:#f5f5f5;position: absolute;left: 555px; bottom: 192px;border-radius: 0px;color: grey;padding: 10px;font-weight: bold;border: 1px;font-size: 15px">
                        <br><br>
                        <label id="">Resident</label>
                        <input type="" name="" class="textbox1" style="margin-left: 77px;">&nbsp&nbsp
                        <input type="" name="" class="textbox1"><br><br>
                        <a href="Academics.html">
                            <input type="button" name="" value="Save" class="save">
                        </a>
                    </form>

                </div>
            </div>

        </div>






    </div>
    <script>
        $(function () {
            $("#change").click(function () {
                $("#status").css("background-color", "#f1413b");
                $("#status").text("VERIFY");
                $("#email").removeAttr("disabled");
                $("#status").click(function () {
                    $("#change").attr("disabled", true);
                    $("#status").css("background-color", "#eea553");
                    $("#status").text("VERIFING...");
                    $("#email").attr("disabled", true);



                })
            })
            $("#changemobile").click(function () {
                $("#statusmobile").css("background-color", "#f1413b");
                $("#statusmobile").text("VERIFY");
                $("#mobile").removeAttr("disabled");
                $("#statusmobile").click(function () {
                    $("#changemobile").attr("disabled", true);
                    $("#statusmobile").css("background-color", "#eea553");
                    $("#statusmobile").text("VERIFING...");
                    $("#mobile").attr("disabled", true);



                })
            })

        });
    </script>





</body>



</html>