<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <title>Dashboard</title>
    <meta name="viewport" content="width=device-width, user-scalable=yes">
    <link rel="stylesheet" type="text/css" href="/css/submission_styling.css">
    <link rel="stylesheet" href="/vendors/css/font-awesome.min.css">
    <style>
        a:any-link {
            color: black;
            text-decoration: none;
        }
    </style>

    <%- include ("../partials/header") %>

    <div id="container">
        <div id="notactive">Not Active</div>

        <div id="containsforms">
            <div class="row2">
                <div class="column2 left2">
                    <button class="button51">
                        <div class="column21 left21"><a href="/form/personal"> Personal Details </a></div>
                        <div class="column21 right21">
                            <div class="saved">Saved</div>
                        </div>
                    </button>
                </div>
                <div class="column2 mid2">
                    <button class="button51" style="margin-left: 0px;">
                        <div class="column21 left21"><a href="/form/academic"> Academic Details </a></div>
                        <div class="column21 right21">
                            <div class="saved">Saved</div>
                        </div>
                    </button>
                </div>
                <div class="column2 right2">
                    &nbsp&nbsp
                    <button class="button5"
                        style="margin-left: -7px; background-image: linear-gradient(-90deg,#FF0D6F,#FFB20A); color: white;">
                        <div class="column21 left21" id="current">Submission</div>
                        <div class="column21 right21">
                            <div></div>
                        </div>
                    </button>
                </div>
                <div class="container1">
                    <form method="" action="" id="up-form">
                        <label>Upload photo</label>
                        <input type="file" class="textbox1" id="file1" required>
                        <br>
                        <label>Upload ID</label>&nbsp&nbsp&nbsp
                        &nbsp&nbsp
                        <input type="file" id="file2" class="textbox1" required><br><br><br>
                        <input type="checkbox" name="" required style="box-shadow: 1px 1px 4px 1px #ccc;">
                        &nbsp&nbsp&nbsp
                        I understand and agree all <a href="" style="color: orange "> terms and conditions.</a>
                        <br>
                        <!-- <a href="/response" id="submit">Submit</a> -->
                        <button type="submit" id="submit" style="box-shadow: 1px 1px 4px 1px #ccc;"> Submit </button>
                    </form>
                </div>
            </div>
        </div>
    </div>

    <script src="/scripts/fileupload.js" defer></script>

